      public function Dm_ImpoliteBoast()
      {
         super();
      }
      
      override public function dm_automaticNoisy(param1:int) : void
      {
         if(param1 == Dm_ImpoliteBoast.dm_femaleEfficient)
         {
            Dm_ClammyInnate.dm_pushyTedious(true,Dm_FaithfulCrowded.dm_slipMark(Dm_AdjustmentAnalyze.dm_flockAunt));
            return;
         }
         if(Dm_ImpoliteBoast.dm_jellySpoil == param1)
         {
            return;
         }
         if(param1 == Dm_ImpoliteBoast.dm_balvankaPig)
         {
            Dm_ClammyInnate.dm_saveCracker();
            return;
         }
         if(param1 == Dm_ImpoliteBoast.dm_priceTour)
         {
            Dm_ZooHuge.dm_lookGate();
            return;
         }
      }
      
      override public function dm_spotlessSense() : void
      {
      }
   }
